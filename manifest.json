{
  "manifest_version": 2,

  "name": "SO Chat Commander",
  "description": "This extension will collapse all outboxed items in Stackoverflow Chat rooms (Images, videos, etc.)!",
  "version": "1.0",
  "content_scripts": [
    {
		"matches" : ["*://chat.stackoverflow.com/*","*://chat.stackexchange.com/*","*://chat.meta.stackexchange.com/*"],
     "js":["thirdParty/jquery-2.2.2.min.js", "background.js"]
    }
  ],
  "browser_action": {
   "default_icon": "icon.png"
  }
}